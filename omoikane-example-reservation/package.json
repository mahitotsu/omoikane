{
  "name": "omoikane-example-reservation",
  "version": "1.0.0",
  "description": "Omoikane Example Reservation - 来店予約管理ユースケースのコンパクト実装例",
  "main": "dist/index.js",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "dev": "bun --watch src/index.ts",
    "type-check": "bun --bun tsc --noEmit",
    "auto-build": "bun ../omoikane-metamodel/scripts/auto-build.ts",
    "generate-references": "bun ../omoikane-metamodel/scripts/generate-typed-references.ts",
    "convert-references": "bun ../omoikane-metamodel/scripts/convert-references.ts",
    "quality-assessment": "bun ../omoikane-metamodel/scripts/quality-assessment.ts .",
    "lint": "eslint . --ext .ts --ignore-path ../.gitignore",
    "lint:fix": "eslint . --ext .ts --ignore-path ../.gitignore --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\" --ignore-path ../.gitignore",
    "format:check": "prettier --check \"**/*.{ts,json,md}\" --ignore-path ../.gitignore",
    "quality": "bun run format && bun run lint:fix && bun run type-check",
    "test": "bun test"
  },
  "keywords": [
    "typescript",
    "usecase",
    "requirements",
    "reservation",
    "example",
    "omoikane"
  ],
  "author": "omoikane",
  "license": "MIT",
  "dependencies": {
    "omoikane-metamodel": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ]
}
